create table CBRC_AML_RET_NAME
(
  seq_no       VARCHAR2(10),
  notice_date  VARCHAR2(10),
  notice_name  VARCHAR2(200),
  san_tar_name VARCHAR2(500),
  alias_name   VARCHAR2(1000),
  split_type   VARCHAR2(2),
  key_words    VARCHAR2(500),
  ret_name     VARCHAR2(500),
  status       VARCHAR2(2),
  bak          VARCHAR2(200)
)
