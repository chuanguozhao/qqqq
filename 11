--O_GXOIETWP
create table O_GXOIETWP (
       GFJ3A      number(2),
       GFJMA      varchar2(9),    
       GFO5A      number(3),
       GFZRMA     number(5),
       GFZRNA     varchar2(3),    
       GFX2AA     number(2),
       GFX0YA     number(3),
       GFZROA     number(7),
       GFZRPA     varchar2(9),    
       GFX12A     number(5),
       GFX0CA     varchar2(21),   
       GFWS3A     number(2),
       GFPQA      number(13),
       GFPRA      number(13),
       GFPSA      number(13),
       GFPTA      number(13),
       GFX13A     number(7),
       GFX0NA     number(7),
       GFPUA      number(2),
       GFPVA      number(13),
       GFPWA      number(2),
       GFPXA      number(13),
       GFX15A     varchar2(75)

);
-- Add comments to the table 
comment on table O_GXOIETWP
  is 'O_GXOIETWP';
-- Add comments to the columns 
comment on column O_GXOIETWP.GFJ3A is 'IFCT Company Number';
comment on column O_GXOIETWP.GFJMA is 'IFCT Branch Code';
comment on column O_GXOIETWP.GFO5A is 'IFCT Export AFC Number'; 
comment on column O_GXOIETWP.GFZRMA is 'IFCT Client No'; 
comment on column O_GXOIETWP.GFZRNA is 'IFCT Business Type'; 
comment on column O_GXOIETWP.GFX2AA is 'IFCT Currency Code'; 
comment on column O_GXOIETWP.GFX0YA is 'IFCT Import AFC Number'; 
comment on column O_GXOIETWP.GFZROA is 'IFCT Debtor Number '; 
comment on column O_GXOIETWP.GFZRPA is 'IFCT Debtor Currency Code'; 
comment on column O_GXOIETWP.GFX12A is 'IFCT Item Number'; 
comment on column O_GXOIETWP.GFX0CA is 'IFCT Document Number'; 
comment on column O_GXOIETWP.GFWS3A is 'IFCT Document Type'; 
comment on column O_GXOIETWP.GFPQA is 'IFCT Amount'; 
comment on column O_GXOIETWP.GFPRA is 'IFCT Client Currency Equivalent'; 
comment on column O_GXOIETWP.GFPSA is 'IFCT Balance'; 
comment on column O_GXOIETWP.GFPTA is 'IFCT Exchange Rate'; 
comment on column O_GXOIETWP.GFX13A is 'IFCT Document Date'; 
comment on column O_GXOIETWP.GFX0NA is 'IFCT Due Date'; 
comment on column O_GXOIETWP.GFPUA is 'IFCT Funding Disapproval Code'; 
comment on column O_GXOIETWP.GFPVA is 'IFCT Funding Disapproval Amount'; 
